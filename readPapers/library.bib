@article{Madhavan2002,
abstract = {An extended Kalman filter (EKF)-based algorithm for the localization of a team of robots is described in this paper. The distributed EKF localization scheme is straightforward in that the individual robots maintain a pose estimate using EKFs that are local to every robot. We then show how these results can be extended to perform heterogeneous cooperative localization in the absence or degradation of absolute sensors aboard the team members. The proposed algorithms are implemented using field data obtained from a team of ATRV-mini robots traversing on uneven outdoor terrain.},

journal = {IEEE Int. Conf. Intell. Robot. Syst.},
keywords = {Mapping,Motion and Path Planning,Path Planning for Multiple Mobile Robots or Agents},
pages = {3254--3259},
title = {{Planning for multi-robot exploration with multiple objective utility functions}},
year = {2011}
}
@article{Howard2006,
abstract = {We describe the design and experimental validation of a large heterogeneous mobile robot team built for the DARPA Software for Distributed Robotics (SDR) program. The core challenge for the SDR program was to develop a multi-robot system capable of carrying out a specifc mission: to deploy a large number of robots into an unexplored building, map the building interior, detect and track intruders, and transmit all of the$\backslash$nabove information to a remote$\backslash$noperator. To satisfy these requirements, we  developed a heterogeneous robot team consisting$\backslash$nof approximately 80 robots. We sketch the key technical elements of this team, focusing on the novel aspects, and present selected results from supervised experiments conducted in a 600m2 indoor environment.},
